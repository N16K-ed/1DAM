package franciscoJavier;

public class Hábitat {
	private String nombre;
	private String lugar;
	
	public Hábitat() {
		super();
	}
	
	public Hábitat(String nombre) {
		super();
		this.nombre = nombre;
	}
	
	public Hábitat(String nombre, String lugar) {
		super();
		this.nombre = nombre;
		this.lugar = lugar;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public String getLugar() {
		return lugar;
	}

	public void setLugar(String lugar) {
		this.lugar = lugar;
	}

	@Override
	public String toString() {
		return "Hábitat [nombre=" + nombre + ", lugar=" + lugar + "]";
	}
	
	
	
}
